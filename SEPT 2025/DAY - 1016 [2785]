** 2785. Sort Vowels in a String **

# Approach 1:
class Solution:
    def sortVowels(self, s: str) -> str:
        vowels = set("aeiouAEIOU")
        
        collected = [ch for ch in s if ch in vowels]
        
        collected.sort()
        
        result = []
        j = 0 
        for ch in s:
            if ch in vowels:
                result.append(collected[j])
                j += 1
            else:
                result.append(ch)
        
        return "".join(result)

# Approach 2:
class Solution:
    def sortVowels(self, s: str) -> str:
        vowelsConstant = {"A","E","I","O","U", "a", "e", "i", "o", "u"}
        vowels = []
        slist = []
        for ch in s:
            slist.append(ch)
            if ch in vowelsConstant:
                vowels.append(ch)
        vowels.sort()
        j = 0
        for i in range(len(slist)):
            if slist[i] in vowelsConstant:
                slist[i] = vowels[j]
                j += 1

        return "".join(slist)
